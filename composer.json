{
  "name": "jw_301/jw_watermark",
  "type": "typo3-cms-extension",
  "description": "Watermark Extension for TYPO3, adding a ViewHelper that creates watermarks.",
  "homepage": "https://github.com/JW301/jw_watermark",
  "license": "MIT",
  "keywords": [
    "TYPO3 CMS",
    "watermark",
    "viewhelper"
  ],
  "support": {
    "issues": "https://github.com/JW301/jw_watermark/issues"
  },
  "require": {
    "typo3/cms-core": "^8.7 || ^9.5 || ^10.4"
  },
  "autoload": {
    "psr-4": {
      "Jw301\\Watermark\\": "Classes"
    },
    "classmap": [
      "Classes"
    ]
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "jw_watermark"
    }
  }
}
